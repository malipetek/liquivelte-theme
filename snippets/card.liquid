	{%- liquid
			
			
			assign left = null
			assign top = null
			assign width = null
			assign height = null
			-%}
	{%- liquid 
			assign slot_default_card = null
			
			-%}
	
	{%- unless sub_include -%} 
	</div>
	{% assign component_include_count = 0 %}
	{%- endunless -%}
	