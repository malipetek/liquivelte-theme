	{%- liquid
			
			
			assign left = null
			assign top = null
			assign width = null
			assign height = null
			-%}
	{%- liquid
	assign lv_props_arr = props | split: '-prsp-'
	for lv_prop in lv_props_arr
	assign lv_prop_and_value = lv_prop | split: '-kvsp-'
	assign lv_propName = lv_prop_and_value | first
	if lv_prop_and_value.size > 1
		assign lv_propValue = lv_prop_and_value | last | remove: '"'
	else
		assign lv_propValue = ''	
	endif
	if lv_propValue == '0'
		assign lv_propValue = lv_propValue | times: 1
	endif
	if lv_propValue contains '{' and lv_propValue contains '}'
	assign lv_propValue = lv_propValue | remove: '{' | remove: '}'
	assign lv_propValueArr = lv_propValue | split: ','
	for lv_entry in lv_propValueArr
	assign lv_key = lv_entry | split: ':' | first | strip | remove: '"'
	assign lv_value = lv_entry | split: ':' | last | strip | remove: '"'
	assign composite_key = lv_propName | append: '_' | append: lv_key
	
	endfor
	endif
	if lv_propName == 'left' and left == blank
		assign left = lv_propValue
	endif
	if lv_propName == 'top' and top == blank
		assign top = lv_propValue
	endif
	if lv_propName == 'width' and width == blank
		assign width = lv_propValue
	endif
	if lv_propName == 'height' and height == blank
		assign height = lv_propValue
	endif
	if lv_propName == 'settings' and settings == blank
		assign settings = lv_propValue
	endif
	if lv_propName == 'card_classes' and card_classes == blank
		assign card_classes = lv_propValue
	endif
	endfor
	-%}
	
			{%- liquid
				assign slot_contents_and_values = slot_contents | split: '-scs-'
				for content_and_value in slot_contents_and_values
					assign module_and_slotname = content_and_value | split: '-scvs-' | first
					assign lv_module = module_and_slotname | split: '-smns-' | first
					assign lv_name = module_and_slotname | split: '-smns-' | last
					assign lv_value = content_and_value | split: '-scvs-' | last
					
					if lv_module == 'card'
						assign slot_default_card = lv_value | strip
					endif
				endfor
				-%}
	{%- unless sub_include -%} 
	<div  class="liquivelte-component card svelte-1gss31k" data-liquivelte-component-name="card.liquivelte">
		{% assign component_include_count_slot_offset = 0 %}
		{% assign component_include_count = 0 %}
		{% assign component_include_depth = 0 %}
		<script> window.cicR = 0; </script>
	{%- endunless -%}
	{% liquid 
  assign card_classes = ''
  if settings.card_vertical == 'center'
    assign card_classes = card_classes | append: ' flex flex-col justify-center'
  elsif settings.card_vertical == 'bottom'
    assign card_classes = card_classes | append: ' flex flex-col justify-end'
  elsif settings.card_vertical == 'even'
    assign card_classes = card_classes | append: ' flex flex-col justify-between'
  endif
%}


<div  class="card {{ card_classes }} svelte-1gss31k" 
      style="--left: {{ settings.card_left }}%;
            --top: {{ settings.card_top }}%;
            --width: {{ settings.card_width }}%;
            --height: {{ settings.card_height }}%; 
            --left_mobile: {{ settings.card_left_mobile }}%; 
            --top_mobile: {{ settings.card_top_mobile }}%;
            --width_mobile: {{ settings.card_width_mobile }}%;
            --height_mobile: {{ settings.card_height_mobile }}%;">
  
		{%- if slot_default_card != blank -%}
			{{- slot_default_card -}}
		{%- endif -%}
	
</div>

	
						
		
	<script type="text/noscript"  class="instance-data svelte-1gss31k">
		{% assign component_include_count = component_include_count | plus: 1 %}
		{
	"settings" : {{ settings | json }} , "card_classes" : {{ card_classes | json }}
	
	
		,
		"forloop_index": {{ forloop.index0 | default: 0 | json }},
		"component_index": {{ component_include_count | plus: component_include_depth }},
		"component_name": "card"
	}</script>
	
