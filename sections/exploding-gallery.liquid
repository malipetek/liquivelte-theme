{% schema %}{
  "name": "Scroll Gallery",
  "tag": "section",
  "class": "product-section spaced-section",
  "settings": [
    {
      "id": "title",
      "type": "text",
      "label": "Title"
    },
    {
      "id": "subtitle",
      "type": "text",
      "label": "Subtitle"
    },
    {
      "type": "html",
      "id": "footnote",
      "label": "Footnote"
    },
    {
      "id": "newsetting",
      "label": "New Setting",
      "type": "text"
    }
  ],
  "blocks": [
    {
      "type": "image",
      "name": "Image",
      "settings": [
