
<script>
export let src;
export let aspect_ratio;
export let sizes;
export let classes;
let srcset = false;
</script>
{% if srcset %}
  <img
    srcset="{{- srcset -}}"
    sizes="{{- sizes -}}"
    class="{{- classes -}}"
    src="{{- src -}}"
    alt="{{- alt | escape -}}"
    width="{{- imageUrl.width -}}"
    height="{{- imageUrl.width | divided_by: aspect_ratio | ceil -}}"
  >
{% else %}
<img
  class="{{- classes -}}"
  src="{{- src -}}"
  alt="{{- alt | escape -}}"
>
{% endif %}

